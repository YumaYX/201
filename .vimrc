nmap <Esc><Esc> :nohlsearch<CR><Esc>
set autoread
set cursorline
set expandtab
set fenc=utf-8
set hidden
set hlsearch
set laststatus=2
set list listchars=tab:\>\-
set shiftwidth=2
set showcmd
set showmatch
set smartcase
set smartindent
set tabstop=2
set wildmode=list:longest
set wrapscan
syntax enable
